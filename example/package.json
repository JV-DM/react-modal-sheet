{
  "name": "example",
  "version": "1.0.0",
  "license": "MIT",
  "homepage": "https://temzasse.github.io/react-modal-sheet",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf dist && vite build --base /react-modal-sheet/",
    "preview": "vite preview --base /react-modal-sheet/",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "format:check": "biome format ./src",
    "format:write": "biome format --write ./src",
    "lint:check": "biome lint --error-on-warnings ./src",
    "lint:write": "biome lint --error-on-warnings  --write ./src"
  },
  "dependencies": {
    "motion": "11.15.0",
    "react": "19.0.0",
    "react-aria": "3.36.0",
    "react-dom": "19.0.0",
    "react-icons": "5.4.0",
    "react-modal-sheet": "file:.yalc/react-modal-sheet",
    "react-router": "7.1.1",
    "react-stately": "3.34.0",
    "styled-components": "6.1.13"
  },
  "devDependencies": {
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@vitejs/plugin-react": "4.3.4",
    "gh-pages": "3.1.0",
    "typescript": "5.7.2",
    "vite": "6.0.6"
  }
}
